@using MVCKutuphanesi.Models.Entity
@model List<TBLMESAJLAR>
@{
    ViewBag.Title = "GidenMesajlar";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}
<br />
<h2>Giden Mesajlar</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Alıcı</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>İşlem</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ALICI</td>
            <td>@x.KONU</td>
            <td>@x.ICERIK</td>
            <td>@x.TARIH</td>
        </tr>

    }
</table>
