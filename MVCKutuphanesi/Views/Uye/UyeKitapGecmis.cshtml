@using MVCKutuphanesi.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<br />
<h3>@ViewBag.k1</h3>
<br />
<table class="table table-bordered">
    <tr>
        <th>Kitap Adı</th>
        <th>Personel</th>
        <th>Alış Taarihi</th>
        <th>İade Tarihi</th>
    </tr>
    @foreach (var x in Model) {
        <tr>
            <td>@x.TBLKITAP.AD</td>
            <td>@x.TBLPERSONEL.PERSONEL</td>
            <td>@Convert.ToDateTime(x.ALISTARIHI).ToString("dd/MM/yyyy")</td>
            <td>@Convert.ToDateTime(x.UYEGETIRTARIH).ToString("dd/MM/yyyy")</td>
        </tr>
    }
</table>

